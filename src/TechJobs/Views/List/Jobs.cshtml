<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@if (ViewBag.jobs != null)
{


    @foreach (Dictionary<string, string> columns in ViewBag.jobs)
    {
        <table class="job-listing">

            @foreach (KeyValuePair<string, string> column in columns)
            {
                <tr>
                    <td>@column.Key</td>
                    <td>@column.Value</td>
                </tr>
            }
        </table>
    }

}
    
